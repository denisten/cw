  "lint-staged": {
    "*.{ts, tsx}": [
      "eslint --cache --ext .js,.jsx,.ts,.tsx src --max-warnings=1",
      "prettier --write",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@types/debounce": "^1.2.0",
    "@types/dragscroll": "0.0.0",
    "@types/jest": "^25.2.1",
